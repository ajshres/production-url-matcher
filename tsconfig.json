{
    "compilerOptions": {
        "baseUrl": ".",
        "allowSyntheticDefaultImports": true,
        "downlevelIteration": true,
        "moduleResolution": "Node",
        "declaration": false,
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "paths": {
            "services/*": ["./application/core/services/*"],
            "@base":["./node_modules"]
        }
    }
}